<Window x:Class="DemoSpeed_Orlov.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoSpeed_Orlov"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="6*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Vertical">
            <TextBlock Background="LightBlue" Text="Складской учёт" FontWeight="Bold" FontSize="20px"></TextBlock>
            <StackPanel Orientation="Horizontal">
                <Button Height="20px" HorizontalContentAlignment="Stretch" Margin="5px" VerticalAlignment="Top" Click="Button_Click">sssssssssssssssssssssssssssss</Button>
                <Button Height="20px" HorizontalContentAlignment="Stretch" Margin="5px" VerticalAlignment="Top">ssad</Button>
            </StackPanel>
        </StackPanel>
        <Frame x:Name="AllData" Grid.Row="1" NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
